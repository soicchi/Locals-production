[
  {
    "name": "front-container",
    "image": "654137446526.dkr.ecr.ap-northeast-1.amazonaws.com/locals-app-front-repo",
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "front",
        "awslogs-group": "/ecs/front"
      }
    },
    "portMapping": [
      {
        "protocol": "tcp",
        "containerPort": 80
      }
    ],
    "command": ["yarn", "start"],
    "environment": [
      {
        "name": "NODE_ENV",
        "value": "production"
      },
      {
        "name": "API_URL",
        "value": "https://local-s-app.com"
      }
    ]
  }
]
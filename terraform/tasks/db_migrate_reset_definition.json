[
  {
    "name": "back-container",
    "image": "654137446526.dkr.ecr.ap-northeast-1.amazonaws.com/locals-app-back-repo",
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "db-migrate-reset",
        "awslogs-group": "/ecs/db-migrate-reset"
      }
    },
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 3000
      }
    ],
    "command": ["bundle", "exec", "rails", "db:migrate:reset"],
    "environment": [
      {
        "name": "RAILS_ENV",
        "value": "production"
      },
      {
        "name": "API_DOMAIN",
        "value": "https://local-s-app.com"
      },
      {
        "name": "DISABLE_DATABASE_ENVIRONMENT_CHECK",
        "value": "1"
      }
    ],
    "secrets": [
      {
        "name": "RAILS_MASTER_KEY",
        "valueFrom": "rails-master-key"
      },
      {
        "name": "DB_PASSWORD",
        "valueFrom": "db-password"
      },
      {
        "name": "DB_USERNAME",
        "valueFrom": "db-username"
      },
      {
        "name": "DB_NAME",
        "valueFrom": "db-name"
      },
      {
        "name": "DB_HOST",
        "valueFrom": "db-host"
      }
    ]
  }
]